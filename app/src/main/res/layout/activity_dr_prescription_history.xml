<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".DrPrescriptionHistory">


    <TableLayout
        android:id="@+id/tableLayout3"
        android:layout_width="match_parent"
        android:layout_height="115dp"
        android:background="@color/colorPrimaryDark"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <TableRow>

            <TextView
                android:id="@+id/textView3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:padding="10dp"
                android:text="Height"
                android:textSize="20dp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/textView4"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_gravity="center"
                android:padding="10dp"
                android:text="Weight"
                android:textSize="20dp"
                android:textStyle="bold" />

            <TextView
                android:layout_width="90dp"
                android:layout_height="match_parent"
                android:layout_gravity="center"
                android:padding="10dp"
                android:text="AGE"
                android:textSize="20dp"
                android:textStyle="bold" />

            <TextView
                android:layout_width="86dp"
                android:layout_height="match_parent"
                android:layout_gravity="center"
                android:padding="10dp"
                android:text="BP"
                android:textSize="20dp"
                android:textStyle="bold" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_gravity="center"
                android:padding="10dp"
                android:text="Blood Group"
                android:textSize="20dp"
                android:textStyle="bold" />

            <TextView
                android:layout_width="121dp"
                android:layout_height="match_parent"
                android:layout_gravity="center"
                android:padding="10dp"
                android:text="Gender"
                android:textSize="20dp"
                android:textStyle="bold" />
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TextView
                android:id="@+id/height"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_gravity="center"
                android:padding="10dp"
                android:text=""
                android:textSize="20dp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/weight"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_gravity="center"
                android:padding="10dp"
                android:text=""
                android:textSize="20dp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/age"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_gravity="center"
                android:padding="10dp"
                android:text=""
                android:textSize="20dp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/bp"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_gravity="center"
                android:padding="10dp"
                android:text=" "
                android:textSize="20dp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/bg"
                android:layout_width="116dp"
                android:layout_height="match_parent"
                android:layout_gravity="center"
                android:hint=" "
                android:padding="10dp"
                android:textSize="20dp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/gender"
                android:layout_width="48dp"
                android:layout_height="match_parent"
                android:layout_gravity="center"
                android:padding="10dp"
                android:text=""
                android:textSize="20dp"
                android:textStyle="bold" />
        </TableRow>
    </TableLayout>

    <TextView
        android:id="@+id/textView13"
        android:layout_width="109dp"
        android:layout_height="28dp"
        android:layout_marginStart="10dp"
        android:layout_marginEnd="55dp"
        android:layout_marginBottom="29dp"
        android:gravity="center"
        android:text="Name"
        android:textSize="15dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/medc1"
        app:layout_constraintEnd_toStartOf="@+id/textView12"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="300dp"
        android:layout_height="31dp"
        android:layout_marginEnd="56dp"
        android:layout_marginBottom="24dp"
        android:gravity="center"
        android:text="Medicine Time"
        android:textSize="15dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/medc1_time"
        app:layout_constraintEnd_toStartOf="@+id/textView11" />


    <TextView
        android:id="@+id/textView11"
        android:layout_width="48dp"
        android:layout_height="31dp"
        android:layout_marginEnd="10dp"
        android:layout_marginBottom="29dp"
        android:gravity="center"
        android:text="QTY"
        android:textSize="15dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/qty1"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/medc1"
        android:layout_width="115dp"
        android:layout_height="30dp"
        android:layout_marginStart="10dp"
        android:layout_marginEnd="49dp"
        android:layout_marginBottom="32dp"
        android:gravity="center"
        android:hint="Medcn 1"
        android:textSize="15dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/medc2"
        app:layout_constraintEnd_toStartOf="@+id/medc1_time"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/medc1_time"
        android:layout_width="300dp"
        android:layout_height="30dp"
        android:layout_marginEnd="64dp"
        android:layout_marginBottom="31dp"
        android:gravity="center"
        android:hint="Medicine time"
        android:textSize="15dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/medc2_time"
        app:layout_constraintEnd_toStartOf="@+id/qty1" />

    <TextView
        android:id="@+id/qty1"
        android:layout_width="41dp"
        android:layout_height="30dp"
        android:layout_marginEnd="10dp"
        android:layout_marginBottom="32dp"
        android:gravity="center"
        android:hint="QTY"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/qty2"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/medc2"
        android:layout_width="115dp"
        android:layout_height="30dp"
        android:layout_marginStart="10dp"
        android:layout_marginEnd="49dp"
        android:layout_marginBottom="29dp"
        android:gravity="center"
        android:hint="Medcn 2"
        android:textSize="15dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/medc3"
        app:layout_constraintEnd_toStartOf="@+id/medc2_time"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/medc2_time"
        android:layout_width="300dp"
        android:layout_height="30dp"
        android:layout_marginEnd="62dp"
        android:layout_marginBottom="30dp"
        android:gravity="center"
        android:hint="Medicine time "
        android:textSize="15dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/medc3_time"
        app:layout_constraintEnd_toStartOf="@+id/qty2" />


    <TextView
        android:id="@+id/qty2"
        android:layout_width="41dp"
        android:layout_height="30dp"
        android:layout_marginEnd="10dp"
        android:layout_marginBottom="29dp"
        android:gravity="center"
        android:hint="QTY"
        app:layout_constraintBottom_toTopOf="@+id/qty3"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/medc3"
        android:layout_width="115dp"
        android:layout_height="30dp"
        android:layout_marginStart="10dp"
        android:layout_marginEnd="51dp"
        android:layout_marginBottom="450dp"
        android:gravity="center"
        android:hint="Medcn 3"
        android:textSize="15dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/medc3_time"
        app:layout_constraintStart_toStartOf="parent" />


    <TextView
        android:id="@+id/medc3_time"
        android:layout_width="300dp"
        android:layout_height="30dp"
        android:layout_marginEnd="62dp"
        android:layout_marginBottom="57dp"
        android:gravity="center"
        android:hint="Medicine time "
        android:textSize="15dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/remar"
        app:layout_constraintEnd_toStartOf="@+id/qty3" />

    <TextView
        android:id="@+id/qty3"
        android:layout_width="41dp"
        android:layout_height="30dp"
        android:layout_marginEnd="10dp"
        android:layout_marginBottom="450dp"
        android:gravity="center"
        android:hint="QTY"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/remar"
        android:layout_width="578dp"
        android:layout_height="86dp"
        android:layout_marginStart="16dp"
        android:layout_marginEnd="7dp"
        android:layout_marginBottom="307dp"
        android:text="TextView"
        android:textSize="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>